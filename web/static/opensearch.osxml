<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
    <ShortName>Seekly</ShortName>
    <Description>Search Seekly.org</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <LongName>Seekly Seach</LongName>
    <Image width="16" height="16">https://seekly.org/favicon.png</Image>
    <Url type="text/html" method="get" template="https://seekly.org/search?q={searchTerms}"/>
    <Url type="application/x-suggestions+json" method="GET" template="https://seekly.org/suggest/">
        <Param name="q" value="{searchTerms}"/>
    </Url>
</OpenSearchDescription>
